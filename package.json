{
  "name": "lambda-taggable-s3-event-listener",
  "version": "1.0.0",
  "description": "Listens for the create event in s3 bucket and triggers the indexer lambdas",
  "main": "index.js",
  "scripts": {
    "test": "semistandard && npm run coverage",
    "invoke": "node invoke.js",
    "deploy": "node ./node_modules/dpl/dpl.js",
    "nocov": "node ./node_modules/.bin/mocha test/*.test.js",
    "semistandard": "semistandard --verbose | snazzy",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/ --report lcov -- -R spec"
  },
  "author": "Pascal & Pals",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/lambda-taggable-s3-event-listener.git"
  },
  "devDependencies": {
    "aws-lambda-test-utils": "^1.0.2",
    "aws-sdk": "^2.3.6",
    "aws-sdk-mock": "^1.0.10",
    "dpl": "^3.0.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "pre-commit": "^1.1.2",
    "semistandard": "^7.0.5",
    "snazzy": "^3.0.0"
  },
  "files_to_deploy": [
    "package.json",
    "index.js",
    "/lib",
    ".env"
  ],
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "afterEach"
    ]
  },
  "pre-commit": [
    "semistandard",
    "coverage"
  ],
  "dependencies": {
    "aws-lambda-helper": "^2.6.0",
    "env2": "^2.0.7"
  }
}
